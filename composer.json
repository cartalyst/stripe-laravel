{
    "name": "cartalyst/stripe-laravel",
    "description": "Laravel 5.4 integration for the Cartalyst Stripe package.",
    "keywords": [
        "php",
        "cartalyst",
        "laravel",
        "stripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Cartalyst LLC",
            "email": "help@cartalyst.com",
            "homepage": "https://cartalyst.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "5.4.*",
        "cartalyst/stripe": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "autoload": {
        "psr-4": { "Cartalyst\\Stripe\\Laravel\\": "src/" }
    },
    "extra": {
        "component": "implementation",
        "branch-alias": {
            "dev-master": "6.0.x-dev"
        }
    },
    "minimum-stability": "stable"
